<?xml version="1.0" encoding="UTF-8"?>
<persistence version="1.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
	<persistence-unit name="ShopForKidsEJB" transaction-type="JTA">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		<jta-data-source>jdbcshop</jta-data-source>
		<class>com.oracle.shopforkids.entities.Continent</class>
		<class>com.oracle.shopforkids.entities.Country</class>
		<class>com.oracle.shopforkids.entities.Fournisseur</class>
		<class>com.oracle.shopforkids.entities.Oneorder</class>
		<class>com.oracle.shopforkids.entities.Product</class>
		<class>com.oracle.shopforkids.entities.ShAddress</class>
		<class>com.oracle.shopforkids.entities.ShArticle</class>
		<class>com.oracle.shopforkids.entities.ShCartecredit</class>
		<class>com.oracle.shopforkids.entities.ShCatalog</class>
		<class>com.oracle.shopforkids.entities.ShCategory</class>
		<class>com.oracle.shopforkids.entities.ShMyrole</class>
		<class>com.oracle.shopforkids.entities.ShOrder</class>
		<class>com.oracle.shopforkids.entities.ShRegion</class>
		<class>com.oracle.shopforkids.entities.ShUser</class>
		<class>com.oracle.shopforkids.entities.Stock</class>
		<class>com.oracle.shopforkids.entities.Usergrantrole</class>
		<class>com.oracle.shopforkids.entities.Warehouse</class>
		<properties>
			<property name="eclipselink.target-server" value="WebLogic_10"/>
		</properties>
	</persistence-unit>
</persistence>
